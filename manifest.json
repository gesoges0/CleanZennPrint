{
  "manifest_version": 3,
  "name": "Zenn Print Helper",
  "version": "1.0",
  "description": "Zennの記事をきれいに印刷するための拡張機能",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://zenn.dev/*"],
  "action": {
    "default_title": "Print Zenn Article"
  },
  "background": {
    "service_worker": "background.js"
  }
}

